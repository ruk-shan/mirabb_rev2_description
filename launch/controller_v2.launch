<?xml version="1.0"?>
<launch>

  <include file="$(find mirabb_rev2_description)/launch/gazebo.launch" />
    
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find mirabb_rev2_description)/launch/controller.yaml" 
    command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" ns="/loadcell" 
    args="joint_right_drive_wheel_to_base_position_controller 
          joint_left_drive_wheel_to_base_position_controller
          joint_abb_1_to_abb_0_position_controller
          joint_abb_2_to_abb_1_position_controller
          joint_abb_3_to_abb_2_position_controller
          joint_abb_4_to_abb_3_position_controller
          joint_abb_5_to_abb_4_position_controller
          joint_abb_6_to_abb_5_position_controller"/>


